{
  "name": "@consolelabs/mochi-ui",
  "version": "0.0.2-rc.1",
  "description": "Mochi UI for profiles, numbers, texts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup src/index.ts --format cjs,esm --dts",
    "beta": "VERSION=$(pnpm version prerelease --preid=rc) && pnpm publish && pnpm dist-tag add @consolelabs/mochi-ui@\"$VERSION\" next"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [],
  "author": "tuanddd",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.zip": "^4.2.7",
    "lodash.groupby": "^4.6.0",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "ioredis": "^5.x.x"
  },
  "dependencies": {
    "@consolelabs/mochi-rest": "workspace:*",
    "@bonfida/spl-name-service": "^1.0.9",
    "@solana/web3.js": "^1.78.3",
    "change-case": "^4.1.2",
    "ethers": "^6.x.x",
    "lodash.chunk": "^4.2.0",
    "lodash.merge": "^4.6.2",
    "lodash.zip": "^4.2.0"
  }
}
